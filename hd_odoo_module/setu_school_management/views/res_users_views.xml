<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="setu_school_management_inherited_view_users_form" model="ir.ui.view">
        <field name="name">setu.school.management.inherited.res.users.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <data>
                <xpath expr="//sheet//notebook//page[@name='access_rights']//group//field[@name='company_id']" position="after">
                    <field name="subject_id"/>
                     <field name="school_id"/>
                </xpath>
            </data>
        </field>
    </record>
</odoo>
