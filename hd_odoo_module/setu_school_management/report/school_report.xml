    </record>

    <record id="school_report_from_wizard" model="ir.actions.report">
        <field name="name">Setu School Report</field>
        <field name="model">setu.school</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">setu_school_management.report_school</field>
