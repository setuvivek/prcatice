        <field name="binding_model_id" ref="model_setu_school"/>
        <field name="binding_type">report</field>
    </record>

    <record id="school_report_teacher" model="ir.actions.report">
        <field name="name">Setu School Teacher</field>
        <field name="model">setu.teacher</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">setu_school_management.inherit_mrp_report</field>
        <field name="report_file">setu_school_management.inherit_mrp_report</field>
        <field name="binding_model_id" ref="model_setu_teacher"/>
        <field name="binding_type">report</field>
