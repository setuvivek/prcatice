<odoo>
    <record id="school_report_student" model="ir.actions.report">
        <field name="name">Setu School Student</field>
        <field name="model">setu.student</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">setu_school_management.report_student</field>
        <field name="report_file">setu_school_management.report_student</field>
        <field name="binding_model_id" ref="model_setu_student"/>
