<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="sales_extended_inherited_view_partner_form" model="ir.ui.view">
            <field name="name">sales.extended.inherited.view.partner.form</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                 <xpath expr="//field[@name='vat']" position="after">
                     <field name="setu_credit_limit"/>
                     <field name="setu_reward_points"/>
                </xpath>
                <xpath expr="//notebook/page[@name='internal_notes']" position="after">
                    <page string="Extra Information"/>
                </xpath>
                <xpath expr="//sheet//group//group//field[@name='category_id']" position="after">
                    <field name="is_buyer"/>
                </xpath>

            </field>
        </record>
    </data>
</odoo>