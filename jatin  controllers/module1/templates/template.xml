<odoo>

    <template id="page">
        <div class="table-responsive border-0 " style="width:2000px">
            <t t-set="users" t-value="recordset"/>
            <br/>
            <table class="table table-sm o_main_table"
            >

                <tr>
                    <th name="th_line" class="text-left"
                        style="width:100px;border-bottom:1px solid black;border-right:1px solid black;border-top:1px solid black;border-left:1px solid black">
                        Line nr.
                    </th>
                    <t t-set="line_no" t-value="0"/>
                    <th style="width:600px;border-bottom:1px solid black;border-right:1px solid black;border-top:1px solid black">
                        Name
                    </th>
                    <th style="width:300px;border-bottom:1px solid black;border-right:1px solid black;border-top:1px solid black">
                        Phone
                    </th>
                    <th style="width:600px;border-bottom:1px solid black;border-right:1px solid black;border-top:1px solid black">
                        Email
                    </th>
                    <th style="width:200px;border-bottom:1px solid black;border-right:1px solid black;border-top:1px solid black">
                        Company Type
                    </th>
                    <!--                        <hr/>-->

                </tr>


                <t t-foreach="users" t-as="user">

                    <tr>
                        <t t-set="line_no" t-value="line_no + 1"/>
                        <td name="td_name"
                            style="padding-left:10px;border-left:1px solid black;border-right:1px solid black;border-bottom:1px solid black">
                            <span t-esc="line_no"/>

                        </td>

                        <td style="padding-left:60px;border-bottom:1px solid black;border-right:1px solid black ">
                            <t t-out="user.translated_display_name"></t>

                        </td>
                        <td style="padding-left:60px;border-bottom:1px solid black;border-right:1px solid black ">
                            <t t-out="user.phone"></t>

                        </td>
                        <td style="padding-left:60px;border-bottom:1px solid black;border-right:1px solid black ">
                            <t t-out="user.email"></t>

                        </td>
                        <td style="padding-left:60px;border-bottom:1px solid black;border-right:1px solid black ">
                            <t t-out="user.company_type"></t>
                        </td>
                    </tr>
                </t>

            </table>
            <br/>
        </div>

