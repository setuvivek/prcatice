<odoo>
    <data>

        <record id="setu_student_view_tree" model="ir.ui.view">
            <field name="name">setu.student.view.tree</field>
            <field name="model">setu.student</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree string="setu_student">
                    <field name="first_name"/>
                    <!--                    <field name="address"/>-->
                    <!--                    <field name="city"/>-->
                    <!--                    <field name="state"/>-->
                    <!--                    <field name="phone"/>-->
                    <!--                    <field name="email"/>-->
                    <!--                    <field name="mobile"/>-->
                    <!--                    <field name="roll_no"/>-->
                    <!--                    <field name="dob" widget="date"/>-->
                    <!--                    <field name="teacher_ids"/>-->
                    <!--                    <field name="class_teacher_id"/>-->
                    <!--                    <field name="subject_ids"/>-->
                    <!--                    <field name="school_id"/>-->
                </tree>
            </field>
        </record>

        <record id="setu_student_view_form" model="ir.ui.view">
            <field name="name">setu.student.view.form</field>
            <field name="model">setu.student</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form string="setu_student">
                    <sheet>
                        <group>
                            <group>
                                <field name="first_name"/>
                                <field name="middle_name"/>
                                <field name="last_name"/>
                                <field name="gender" widget="radio"/>
                                <field name="dob" widget="date"/>
                                <field name="phone" widget="phone"/>
                                <field name="email" widget="email"/>
                                <field name="mobile" widget="phone"/>
                                <field name="bloodgroup"/>
                                <field name="weight"/>
                                <field name="height"/>
                                <field name="address"/>
                                <field name="city_id"/>
                                <field name="state_id"/>
                                <field name="terminate_reason"/>
                                <field name="active"/>
                                <field name="standard_id"/>
                                <field name="division_id"/>
                                <field name="medium_id"/>
                                <field name="school_id" options="{'no_open':True, 'no_create': True, 'no_create_edit': True}"/>
                                <field name="admission_date"/>
                                <field name="academic_year_id"/>
                                <field name="caste_id"/>
                                <field name="mother_tongue"/>
                                <field name="roll_no"/>
                                <field name="class_id" options="{'no_open':True, 'no_create': True, 'no_create_edit': True}"/>
                                <field name="class_teacher_id" options="{'no_open':True, 'no_create': True, 'no_create_edit': True}"/>
                                <field name="teacher_ids" widget="many2many_tags" options="{'no_create': True, 'no_create_edit': True}"/>
                                <field name="subject_ids" widget="many2many_tags" options="{'no_create': True, 'no_create_edit': True}"/>
                            </group>
                        </group>

                    </sheet>
                </form>
            </field>
        </record>

        <record id="setu_student_action" model="ir.actions.act_window">
            <field name="name">Student</field>
            <field name="res_model">setu.student</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="setu_student_menu" name="Student" action="setu_student_action" parent="submenu"/>


    </data>
</odoo>