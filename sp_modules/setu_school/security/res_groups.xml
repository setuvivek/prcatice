<?xml version="1.0" encoding="utf-8"?>
<odoo>

     <record model="ir.module.category" id="module_setu_school">
            <field name="name">School</field>
            <field name="sequence">15</field>
        </record>

     <record id="`group_school_user" model="res.groups">
        <field name="name">User</field>
        <field name="category_id" ref="setu_school.module_setu_school"/>
    </record>

    <record id="group_school_manager" model="res.groups">
        <field name="name">Manager</field>
        <field name="category_id" ref="setu_school.module_setu_school"/>
        <field name="implied_ids" eval="[(4, ref('setu_school.group_school_user'))]"/>
    </record>

</odoo>
