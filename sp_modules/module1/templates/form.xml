<odoo>
|('state','=','quatation')('state','=','quatation sent')
    <template id="form">
        <t t-call="website.layout">
            <form action="/form" method="post" data-success_page="/thanks">
<!--                  class="s_website_form form-horizontal container-fluid mt32">-->
                <label>Name:</label>
                <input id="name" type="text" name="name" required="1"/>

                <label>Email:</label>
                <input id="email" type="email" name="email"/>
                <br/>

                <label>Phone:</label>
                <input id="phone" type="text" name="phone"/>
                <br/>

                <br/>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </t>
    </template>
</odoo>


