<odoo>
    <data>
        <record id="setu_grade_views_tree" model="ir.ui.view">
            <field name="name">setu.grade.view.tree</field>
            <field name="model">setu.grade</field>
            <field name="arch" type="xml">
                <tree string="setu_grade">
                    <field name="name"/>
                    <field name="grade_line_ids"/>
                </tree>
            </field>
        </record>
        <record id="setu_grade_views_form" model="ir.ui.view">
            <field name="name">setu.grade.view.form</field>
            <field name="model">setu.grade</field>
            <field name="arch" type="xml">
                <form string="setu_grade">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="grade_line_ids"/>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="setu_grade_action" model="ir.actions.act_window">
            <field name="name">Setu Grade</field>
            <field name="res_model">setu.grade</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="sub_menu1" name="Setu Grade"  parent="main_menu"/>
        <menuitem id="setu_grade_menu" name="Setu Grade" action="setu_grade_action" parent="sub_menu1"/>
    </data>
</odoo>