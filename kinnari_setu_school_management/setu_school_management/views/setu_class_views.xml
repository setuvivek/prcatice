<odoo>
    <data>
        <record id="setu_class_view_tree" model="ir.ui.view">
            <field name="name">setu.class.view.tree</field>
            <field name="model">setu.class</field>
            <field name="arch" type="xml">
                <tree string="class">
                    <field name="name"/>
                    <field name="class_teacher_id1"/>
                    <field name="teacher_ids"/>
                    <field name="student_ids"/>
                    <field name="school_ids"/>
                    <field name="subject_ids"/>
                </tree>
            </field>
        </record>
        <record id="setu_class_view_form" model="ir.ui.view">
            <field name="name">setu.class.view.form</field>
            <field name="model">setu.class</field>
            <field name="arch" type="xml">
                <form string="class">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="class_teacher_id1"/>
                            <field name="teacher_ids"/>
                            <field name="student_ids"/>
                            <field name="school_ids"/>
                            <field name="subject_ids"/>
                        </group>
                        <notebook>
                            <page string="Teachers">
                                <field name="teacher_ids">
                                    <tree string="teachers">
                                        <field name="name"/>
                                        <field name="address"/>
                                        <field name="email"/>
                                        <field name="mobile" optional="hide"/>
                                        <field name="phone" optional="hide"/>
                                        <field name="standard_id"/>
                                        <field name="subject_ids"/>
                                        <field name="school_id"/>
                                        <field name="student_ids"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Students">
                                <field name="student_ids">
                                    <tree string="student">
                                        <field name="first_name"/>
                                        <field name="middle_name"/>
                                        <field name="last_name"/>
                                        <field name="gender"/>
                                        <field name="date_of_birth"/>
                                        <field name="weight"/>
                                        <field name="height"/>
                                        <field name="state"/>
                                        <field name="terminate_reason"/>
                                        <field name="active"/>
                                        <field name="standard_id"/>
                                        <field name="division_id"/>
                                        <field name="medium_id"/>
                                        <field name="school_id"/>
                                        <field name="admission_date"/>
                                        <field name="academic_year_id"/>
                                        <field name="roll_no"/>
                                        <field name="cast_id"/>
                                        <field name="mother_tongue_id"/>
                                    </tree>
                                </field>
                            </page>
                            <page string="Schools">
                                <field name="school_ids">
                                    <tree string="school">
                                        <field name="name"/>
                                        <field name="code"/>
                                        <field name="street"/>
                                        <field name="city"/>
                                        <field name="state_id"/>
                                        <field name="zip"/>
                                        <field name="country_id"/>
                                        <field name="required_age"/>
                                        <field name="school_standard_ids"/>
                                        <field name="email"/>
                                        <field name="phone"/>

                                    </tree>
                                </field>
                            </page>
                            <page string="Subjects">
                                <field name="subject_ids">
                                    <tree string="subject">
                                        <field name="name"/>
                                        <field name="code"/>
                                        <field name="maximum_marks"/>
                                        <field name="minimum_marks"/>
                                        <field name="weightage"/>
                                        <field name="standard_id"/>
                                        <field name="student_ids"/>
                                        <field name="standard_ids"/>
                                        <field name="teacher_ids"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="setu_class_action" model="ir.actions.act_window">
            <field name="name">Setu Class</field>
            <field name="res_model">setu.class</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="sub_class_main_menu" name="Setu Class"  parent="main_menu"/>
        <menuitem id="setu_class_menu" name="Setu Class" action="setu_class_action" parent="sub_class_main_menu"/>



    </data>
</odoo>