<odoo>
    <data>
        <record id="setu_academic_year_views_tree" model="ir.ui.view">
            <field name="name">setu.academic.year.view.tree</field>
            <field name="model">setu.academic.year</field>
            <field name="arch" type="xml">
                <tree string="setu_academic_year">
                    <field name="sequence"/>
                    <field name="name"/>
                    <field name="code"/>
                    <field name="date_start"/>
                    <field name="date_stop"/>
                    <field name="month_ids"/>
                </tree>
            </field>
        </record>
        <record id="setu_academic_year_views_form" model="ir.ui.view">
            <field name="name">setu.academic.year.view.form</field>
            <field name="model">setu.academic.year</field>
            <field name="arch" type="xml">
                <form string="setu_academic_year">
                    <header>
                        <button name="action_create" string="New" type="object"/>
                    </header>
                    <sheet>
                        <group>
                            <field name="sequence"/>
                            <field name="name"/>
                            <field name="code"/>
                            <field name="date_start"/>
                            <field name="date_stop"/>
                            <field name="month_ids"/>

                        </group>
                        <notebook>
                            <page string="Month Details">
                                <field name="month_ids">
                                    <tree string="month">
                                        <field name="name"/>
                                        <field name="code"/>
                                        <field name="date_start"/>
                                        <field name="date_stop"/>
                                        <field name="academic_year_id"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids"/>
                        <field name="activity_ids"/>
                        <field name="message_ids"/>
                    </div>
                </form>
            </field>
        </record>
        <record id="setu_academic_year_action" model="ir.actions.act_window">
            <field name="name">Setu Academic Year</field>
            <field name="res_model">setu.academic.year</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="sub_menu" name="Setu Academic" parent="main_menu"/>
        <menuitem id="setu_academic_year_menu" name="Setu Academic Year" action="setu_academic_year_action"
                  parent="sub_menu"/>
    </data>
</odoo>


