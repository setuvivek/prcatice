<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="setu_grade_view_tree" model="ir.ui.view">
        <field name="name">setu.grade.view.tree</field>
        <field name="model">setu.grade</field>
        <field name="arch" type="xml">
            <tree string="Setu Grade">
                <field name="name"/>

            </tree>
        </field>
    </record>
    <record id="setu_grade_view_form" model="ir.ui.view">
        <field name="name">setu.grade.view.form</field>
        <field name="model">setu.grade</field>
        <field name="arch" type="xml">
            <form string="Setu Grade">
                <sheet>
                    <group>
                        <field name="name"/>
                    </group>
                    <notebook>
                        <page string="Grade Lines">
                            <field name="grade_lines_ids">
                                <tree>
                                    <field name="from_mask"/>
                                    <field name="to_mask"/>
                                    <field name="grade_name"/>
                                    <field name="fail"/>
                                    <field name="grade_id"/>

                                </tree>
                            </field>
                        </page>
                    </notebook>
                </sheet>
            </form>
        </field>
    </record>
    <record id="setu_grade_action" model="ir.actions.act_window">
        <field name="name">Grade</field>
        <field name="res_model">setu.grade</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="grade"
              name="Grade"
              parent="root"
    />
    <menuitem id="setu_grade_menu"
              name="Grade"
              action="setu_grade_action"
              parent="grade"
    />
</odoo>