<odoo>
    <data>
        <record id="setu_student_cast_views_tree" model="ir.ui.view">
            <field name="name">setu.student.cast.view.tree</field>
            <field name="model">setu.student.cast</field>
            <field name="arch" type="xml">
                <tree string="setu_student_cast">
                    <field name="name"/>
                </tree>
            </field>
        </record>
        <record id="setu_student_cast_views_form" model="ir.ui.view">
            <field name="name">setu.student.cast.view.form</field>
            <field name="model">setu.student.cast</field>
            <field name="arch" type="xml">
                <form string="setu_student_cast">
                    <sheet>
                        <group>
                            <field name="name"/>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="setu_student_cast_action" model="ir.actions.act_window">
            <field name="name">Setu Student Cast</field>
            <field name="res_model">setu.student.cast</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="setu_student_cast_menu" name="Setu Student Cast" action="setu_student_cast_action" parent="sub_student_main"/>
    </data>
</odoo>