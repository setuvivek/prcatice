<odoo>
    <data>
        <record id="setu_standard_standard_views_tree" model="ir.ui.view">
            <field name="name">setu.standard.standard.view.tree</field>
            <field name="model">setu.standard.standard</field>
            <field name="arch" type="xml">
                <tree string="setu_standard_standard">
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>
        <record id="setu_standard_standard_views_form" model="ir.ui.view">
            <field name="name">setu.standard.standard.view.form</field>
            <field name="model">setu.standard.standard</field>
            <field name="arch" type="xml">
                <form string="setu_standard_standard">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="code"/>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids"/>
                        <field name="activity_ids"/>
                        <field name="message_ids"/>
                    </div>
                </form>
            </field>
        </record>
        <record id="setu_standard_standard_action" model="ir.actions.act_window">
            <field name="name">Setu Standard Standard</field>
            <field name="res_model">setu.standard.standard</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="setu_standard_standard_menu" name="Setu Standard Standard" action="setu_standard_standard_action"
                  parent="sub_menu_standard"/>
    </data>
</odoo>