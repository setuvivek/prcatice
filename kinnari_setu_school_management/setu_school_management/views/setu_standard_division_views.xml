<odoo>
    <data>
        <record id="setu_standard_division_views_tree" model="ir.ui.view">
            <field name="name">setu.standard.division.view.tree</field>
            <field name="model">setu.standard.division</field>
            <field name="arch" type="xml">
                <tree string="setu_standard_division">
                    <field name="name"/>
                    <field name="code"/>
                </tree>
            </field>
        </record>
        <record id="setu_standard_division_views_form" model="ir.ui.view">
            <field name="name">setu.standard.division.view.form</field>
            <field name="model">setu.standard.division</field>
            <field name="arch" type="xml">
                <form string="setu_standard_division">
                    <sheet>
                        <group>
                            <field name="name"/>
                            <field name="code"/>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="setu_standard_division_action" model="ir.actions.act_window">
            <field name="name">Setu Standard Division</field>
            <field name="res_model">setu.standard.division</field>
            <field name="view_mode">tree,form</field>
        </record>

        <menuitem id="setu_standard_division_menu" name="Setu Standard Division" action="setu_standard_division_action" parent="sub_menu_standard"/>


    </data>
</odoo>