<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="setu_admission_form_view_tree" model="ir.ui.view">
        <field name="name">setu.admission.form.view.tree</field>
        <field name="model">setu.admission.form</field>
        <field name="arch" type="xml">
            <tree string="Setu Admission Form">
                <field name="name"/>
                <field name="class_id"/>
                <field name="address"/>
                <field name="email"/>
                <field name="phone"/>
                <field name="state"/>
                <field name="dob"/>
            </tree>
        </field>
    </record>
    <record id="setu_admission_form_action" model="ir.actions.act_window">
        <field name="name">Admission Form</field>
        <field name="res_model">setu.admission.form</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="setu_admission_form_view_form" model="ir.ui.view">
        <field name="name">setu.admission.form.view.form</field>
        <field name="model">setu.admission.form</field>
        <field name="arch" type="xml">


            <form string="Setu Admission Form">

                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="class_id"/>
                            <field name="address"/>
                            <field name="email"/>
                        </group>
                        <group>
                            <field name="phone"/>
                            <field name="state"/>
                            <field name="dob"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="setu_admission_form_action" model="ir.actions.act_window">
        <field name="name">Admission Form</field>
        <field name="res_model">setu.admission.form</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem id="root"
              name="Setu School Management"
    />
    <menuitem id="root1"
              name="Configuration"
              parent="root"
    />
    <menuitem id="setu_admission_form_menu"
              name="Admission Form"
              action="setu_admission_form_action"
              parent="root1"
    />
</odoo>