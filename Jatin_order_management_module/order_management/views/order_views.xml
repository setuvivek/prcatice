<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="order_view_tree" model="ir.ui.view">
        <field name="name">order.view.tree</field>
        <field name="model">order</field>
        <field name="arch" type="xml">
            <tree string="Order">
                <field name="name"/>
                <field name="no_of_order"/>
                <field name="address"/>
                <field name="date"/>
                <field name="DateTime"/>
                <field name="product_ids"/>
                <field name="pro_many_ids"/>
            </tree>
        </field>
    </record>
    <record id="order_view_form" model="ir.ui.view">
        <field name="name">order.view.form</field>
        <field name="model">order</field>
        <field name="arch" type="xml">
            <form string="Order">
                <sheet>
                    <group>
                        <group>
                            <field name="name"/>
                            <field name="no_of_order"/>
                            <field name="address"/>
                        </group>
                        <group>
                            <field name="address"/>
                            <field name="date"/>
                            <field name="DateTime"/>
                            <field name="product_ids"/>
                            <field name="pro_many_ids"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
     <record id="order_action" model="ir.actions.act_window">
            <field name="name">Order</field>
            <field name="res_model">order</field>
            <field name="view_mode">tree,form</field>
     </record>
    <menuitem id = "root"
              name = "Order"
    />
    <menuitem id="root1"
              name="Order"
              parent="root"
    />
    <menuitem id="order_menu"
              name="Order"
              parent="root1"
              action="order_action"
    />

