<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_partner_extended_inherited_view_partner_form" model="ir.ui.view">
        <field name="name">res.partner.extended.inherited.view_partner_form</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='vat']" position="after">
                <field name="setu_money_credit_limit" optional="show"/>
                <field name="setu_reward_point" optional="show"/>
            </xpath>
        </field>
    </record>
</odoo>